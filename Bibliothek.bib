	date = {1995-11},
	file = {Volltext:/home/peter/snap/zotero-snap/common/Zotero/storage/BWMDNG4F/Wolfe et al. - 1995 - Growth and Yield Sensitivity of Four Vegetable Cro.pdf:application/pdf},
