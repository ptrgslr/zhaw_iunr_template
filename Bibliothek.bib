	langid = {english},
	keywords = {assimilate distribution, Hordeum vulgare, mathematical model, plant adaptation, root growth},
	file = {Full Text PDF:/home/peter/snap/zotero-snap/common/Zotero/storage/FEL2MDZ9/Poluektov und Topazh - 2005 - Calculation of the RootShoot Ratio in the Models .pdf:application/pdf},
}

