	file = {Full Text PDF:/home/peter/snap/zotero-snap/common/Zotero/storage/8MENBBYS/Poeplau et al. - 2021 - Roots are key to increasing the mean residence tim.pdf:application/pdf},
}

@article{bertiOrganicInputQuality2016,
	title = {Organic input quality is more important than its quantity: C turnover coefficients in different cropping systems},
	volume = {77},
	issn = {1161-0301},
	url = {https://www.sciencedirect.com/science/article/pii/S1161030116300508},
	doi = {10.1016/j.eja.2016.03.005},
	shorttitle = {Organic input quality is more important than its quantity},
	abstract = {Annual C input to soil is a major factor affecting soil organic carbon ({SOC}) dynamics. However different types of C-sources can have different behaviour, in relation to their chemical characteristics and how they interact with soil. Root-derived C, in particular, should be more efficient than other organic materials as a result of the physicochemical and biological characteristics of the surrounding environment, leading to a reduction in the C decomposition rate. To test this hypothesis, we considered a long-term experiment underway in Northern Italy since 1962, comparing permanent meadow and 6 different crop rotations over a wide range of nutrient inputs, in both organic and inorganic forms. C inputs from amendments were measured and those from crops were calculated using allometric functions and crop and residues yields. The time evolution of {SOC} was studied through a single-pool, first-order kinetic model, allowing the estimation of humification coefficients for residues, roots, farmyard manure and cattle slurries. The highest value of the humification coefficient was estimated for farmyard manure, which confirmed its high efficiency in stabilising {SOC} content. Root C presented a humification coefficient 1.9 times higher than above-ground plant materials while slurries were intermediate, with a humification coefficient roughly half that of farmyard manure and even lower that of roots. The quality of C input thus seems of fundamental importance for evaluating the sustainability of different cropping systems in terms of {SOC} dynamics.},
	pages = {138--145},
	journaltitle = {European Journal of Agronomy},
	shortjournal = {European Journal of Agronomy},
	author = {Berti, Antonio and Morari, Francesco and Dal Ferro, Nicola and Simonetti, Gianluca and Polese, Riccardo},
	urldate = {2023-05-30},
	date = {2016-07-01},
	langid = {english},
	keywords = {Roots, Soil organic carbon, Crop residues, Humification coefficient, Long-term field experiment, Organic amendments},
	file = {ScienceDirect Full Text PDF:/home/peter/snap/zotero-snap/common/Zotero/storage/AYDX9337/Berti et al. - 2016 - Organic input quality is more important than its q.pdf:application/pdf;ScienceDirect Snapshot:/home/peter/snap/zotero-snap/common/Zotero/storage/YD3P5FIQ/S1161030116300508.html:text/html},
}

@article{huangSoilCarbonPools2011,
	title = {Soil carbon pools, plant biomarkers and mean carbon residence time after afforestation of grassland with three tree species},
	volume = {43},
	issn = {0038-0717},
	url = {https://www.sciencedirect.com/science/article/pii/S0038071711001210},
	doi = {10.1016/j.soilbio.2011.03.008},
	abstract = {Afforestation of grassland has been globally identified as being an important means for creating a sink for atmospheric carbon (C). However, the impact of afforestation on soil C is still poorly understood, due to the paucity of well designed long-term experiments and the lack of investigation into the response of different soil C fractions to afforestation. In addition, little is known about the origins of soil C and soil organic matter ({SOM}) stability after afforestation. In a retrospective study, we measured C mass in the soil light and heavy fractions in the first 10 years after afforestation of grassland with Eucalyptus nitens, Pinus radiata and Cupressus macrocarpa. The results suggest that C mass in the soil heavy fraction remained stable, but the C mass in the light fraction decreased at year 5 under three species. Soil δ13C analysis showed that the decrease in the light fraction may be due to reduced C inputs from grassland species litter and low inputs from the still young trees. After the initial reduction, the recovery of soil C in the light fraction depended on tree species. At year 10, an increase of 33\% in light fraction soil C was observed at the 0–30 cm depth under E. nitens, compared to that under the original grassland (year 0). Planting P. radiata restored light fraction soil C to the original level under grassland, whereas planting C. macrocarpa led to a decrease of 33\%. We concluded that the increase of light fraction soil C between year 5 and 10 is most likely due to C input from tree residues. Most of the increased C was derived from root turnover under pine and from both root and leaf turnover under E. nitens, as indicated by plant C biomarkers such as lignin-derived phenols and suberin and cutin-derived compounds in the 0–5 cm soil layer. Modelling of soil ∆14C‰ suggested that {SOM} had a greater mean residence time at year 10 than year 0 and 5 due to increased relative abundance of recalcitrant plant biopolymers.},
	pages = {1341--1349},
	number = {6},
	journaltitle = {Soil Biology and Biochemistry},
	shortjournal = {Soil Biology and Biochemistry},
	author = {Huang, Zhiqun and Davis, Murray R. and Condron, Leo M. and Clinton, Peter W.},
	urldate = {2023-05-30},
	date = {2011-06-01},
	langid = {english},
	keywords = {Soil organic matter, Afforestation, Carbon, Grassland, Mean residence time, Plant biomarkers, Radiocarbon},
	file = {ScienceDirect Full Text PDF:/home/peter/snap/zotero-snap/common/Zotero/storage/8HNWNPRT/Huang et al. - 2011 - Soil carbon pools, plant biomarkers and mean carbo.pdf:application/pdf;ScienceDirect Snapshot:/home/peter/snap/zotero-snap/common/Zotero/storage/I44E7EZZ/S0038071711001210.html:text/html},
}

