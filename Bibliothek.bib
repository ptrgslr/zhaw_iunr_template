	journaltitle = {Geoderma},
	shortjournal = {Geoderma},
	author = {Herbst, M. and Welp, G. and Macdonald, A. and Jate, M. and Hädicke, A. and Scherer, H. and Gaiser, T. and Herrmann, F. and Amelung, W. and Vanderborght, J.},
	urldate = {2023-05-30},
	date = {2018-03-15},
	langid = {english},
	keywords = {Carbon turnover, Fractionation, Inversion, Particulate organic matter},
	file = {ScienceDirect Full Text PDF:/home/peter/snap/zotero-snap/common/Zotero/storage/UFW2UL4Z/Herbst et al. - 2018 - Correspondence of measured soil carbon fractions a.pdf:application/pdf;ScienceDirect Snapshot:/home/peter/snap/zotero-snap/common/Zotero/storage/ICKVRTC7/S0016706117314581.html:text/html},
